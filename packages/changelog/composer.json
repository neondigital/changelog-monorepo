{
    "name": "neondigital/changelog",
    "version": "0.0.1",
    "description": "A changelog package for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alec Ritson",
            "email": "alec@neondigital.co.uk"
        }
    ],
    "autoload": {
      "psr-4": {
        "Neondigital\\Changelog\\": "src/"
      }
    },
    "extra": {
      "laravel": {
        "providers": [
          "Neondigital\\Changelog\\ChangelogServiceProvider"
        ]
      }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "symplify/monorepo-builder": "^12.2"
    },
    "require": {
        "laravel/framework": "^v11.46|^12.32",
        "spatie/yaml-front-matter": "^2.1",
        "spatie/laravel-data": "^4.17",
        "spatie/laravel-markdown": "^2.7"
    }
}
